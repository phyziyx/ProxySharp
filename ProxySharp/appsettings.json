{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft.AspNetCore": "Warning",
        "System": "Warning"
      }
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  /* The API URL */
  "BASE_URL": "",
  /* API Credentials */
  "API_USERNAME": "",
  "API_PASSWORD": "",
  /* Authorization Timeout - This is supposed to be half, if not less, than of your total regular HTTP requests */
  "AUTH_TIMEOUT": 30
}
