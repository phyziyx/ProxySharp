{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft.AspNetCore": "Warning",
        "System": "Warning"
      }
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  /* The API URL */
  "BaseUrl": "",
  /* API Credentials */
  "Username": "",
  "Password": "",
  /* Authorization Timeout - This is supposed to be half, if not less, than of your total regular HTTP requests */
  "AuthTimeout": 30,
  "Proxy": {
    "Url": "http://127.0.0.1:8888",
    "Username": "",
    "Password": ""
  }
}
